class MyStack {
    private int[] arr;
    private int top;

    public MyStack() {
        arr = new int[1000];
        top = -1;
    }
    // TC O(1) SC O(1)
    public void push(int x) {
        // Your Code
        top++;
        arr[top] = x;
    }
    // TC O(1) SC O(1)
    public int pop() {
        // Your Code
        if(top == -1){
            // Stack is Empty
            return -1;
        }
        return arr[top--];
    }
}
